{
    "name": "sqlquery-inator",
    "version": "0.0.0192",
    "description": "strongly typed SQL queries in typescript",
    "license": "MIT",
    "main": "index.js",
    "types": "index.d.ts",
    "scripts": {
      "clean": "rimraf lib",
      "test": "mocha -r ts-node/register src/**/*.spec.ts",
      "watch": "yarn run test -- --watch-extensions ts --watch -R min",
      "build": "tsc -d -p .",
      "copy": "cpx lib/* ./",
      "prepublishOnly": "yarn build && yarn copy",
      "postpublish": "rm -r *.d.ts *.js *.map lib"
    },
    "dependencies": {
      "@types/mssql": "^4.0.10",
      "@types/mocha": "^5.2.5",
      "@types/chai": "^4.1.6",
      "mocha": "^5.2.0",
      "chai": "^4.2.0",
      "io-ts": "^1.3.1",
      "io-ts-derive-class": "^0.0.21",
      "dbschema-inator": "^0.0.5",
      "moment": "^2.22.2"
    },
    "devDependencies": {
      "rimraf": "^2.6.2",
      "typescript": "^3.1.6",
      "ts-node": "^7.0.1",
      "cpx": "^1.5.0"
    }
  }